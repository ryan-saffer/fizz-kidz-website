---
import Layout from "@/layout/Layout.astro";
import { Image } from "astro:assets";
import purpleWave from "@/assets/images/pages/franchising/v2/purple-wave.png";
import blueWave from "@/assets/images/pages/franchising/v2/blue-wave.png";
import hero from "@/assets/images/pages/franchising/hero.png";
import FranchisingHero from "@/components/image-clips/franchising-hero.astro";
import franchiseChild from "@/assets/images/pages/franchising/v2/franchise-child.jpg";
import talia from "@/assets/images/pages/franchising/v2/talia.jpg";
import { Button } from "@/components/react/ui/button";
import pinkCircle from "@/assets/images/pages/franchising/v2/pink-circle.png";
import yellowCircle from "@/assets/images/pages/franchising/v2/yellow-circle.png";
import greenCircle from "@/assets/images/pages/franchising/v2/green-circle.png";
import purpleCurve from "@/assets/images/pages/franchising/v2/purple-curve.png";
import team from "@/assets/images/pages/franchising/v2/all-staff.jpg";
import clouds from "@/assets/images/pages/franchising/v2/clouds.png";
import check from "@/assets/images/pages/franchising/v2/check.png";
import yellowWaveTop from "@/assets/images/pages/franchising/v2/yellow-wave-top.png";
import yellowWaveBottom from "@/assets/images/pages/franchising/v2/yellow-wave-bottom.png";
import FaqAccordion from "@/components/react/faq-accordion";
import StackedBubbles from "@/components/image-clips/stacked-bubbles.astro";
import faq from "@/assets/images/pages/parties/faq.png";
import graphic1 from "@/assets/images/pages/franchising/v2/graphics/graphic1.png";
import graphic2 from "@/assets/images/pages/franchising/v2/graphics/graphic2.png";
import graphic3 from "@/assets/images/pages/franchising/v2/graphics/graphic3.png";
import graphic4 from "@/assets/images/pages/franchising/v2/graphics/graphic4.png";
import graphic5 from "@/assets/images/pages/franchising/v2/graphics/graphic5.png";
import graphic6 from "@/assets/images/pages/franchising/v2/graphics/graphic6.png";
import graphic7 from "@/assets/images/pages/franchising/v2/graphics/graphic7.png";
import graphic8 from "@/assets/images/pages/franchising/v2/graphics/graphic8.png";
import graphic9 from "@/assets/images/pages/franchising/v2/graphics/graphic9.png";
import graphic10 from "@/assets/images/pages/franchising/v2/graphics/graphic10.png";
import graphic11 from "@/assets/images/pages/franchising/v2/graphics/graphic11.png";
import graphic12 from "@/assets/images/pages/franchising/v2/graphics/graphic12.png";
import graphic13 from "@/assets/images/pages/franchising/v2/graphics/graphic13.png";
import graphic14 from "@/assets/images/pages/franchising/v2/graphics/graphic14.png";
import graphic15 from "@/assets/images/pages/franchising/v2/graphics/graphic15.png";
import birthdayParties from "@/assets/images/pages/franchising/v2/birthday-parties.jpg";
import holidayPrograms from "@/assets/images/pages/franchising/v2/holiday-programs.jpg";
import playLab from "@/assets/images/pages/franchising/v2/play-lab.jpg";
import afterSchool from "@/assets/images/pages/franchising/v2/after-school.jpg";
import activations from "@/assets/images/pages/franchising/v2/activations.jpg";
import schools from "@/assets/images/pages/franchising/v2/schools.jpg";
import topBubble from "@/assets/images/pages/franchising/v2/top-bubble.jpg";
import step1 from "@/assets/images/pages/franchising/v2/step-1.jpg";
import step2 from "@/assets/images/pages/franchising/v2/step-2.jpg";
import step3 from "@/assets/images/pages/franchising/v2/step-3.jpg";
import step4 from "@/assets/images/pages/franchising/v2/step-4.jpg";
import step5 from "@/assets/images/pages/franchising/v2/step-5.jpg";
import step6 from "@/assets/images/pages/franchising/v2/step-6.jpg";
import step7 from "@/assets/images/pages/franchising/v2/step-7.jpg";
import step8 from "@/assets/images/pages/franchising/v2/step-8.jpg";
import studio1 from "@/assets/images/pages/franchising/v2/studio-1.jpg";
import studio2 from "@/assets/images/pages/franchising/v2/studio-2.jpg";
import studio3 from "@/assets/images/pages/franchising/v2/studio-3.jpg";
import studio4 from "@/assets/images/pages/franchising/v2/studio-4.jpg";
import whiteSlime from "@/assets/images/pages/franchising/v2/white-slime.svg";
import FranchisingForm from "@/components/react/forms/franchising-form";
---

<style lang="css">
  /* Disable the header box shadow so the slime works */
  :global(header) {
    box-shadow: none !important;
  }
</style>

<Layout
  title="Kids Party & Event Franchise Opportunities in Melbourne"
  description="Run your own Fizz Kidz studio, and become a part of the Fizz Family!"
>
  <div class="relative bg-[#45D3F3]">
    <Image
      class="absolute w-full"
      src={whiteSlime}
      alt=""
      loading="eager"
      width={80}
      aria-hidden="true"
    />
    <Image
      class="absolute left-[7%] top-[20%] hidden min-[1700px]:block"
      src={graphic12}
      alt=""
      loading="eager"
      width={80}
      aria-hidden="true"
    />
    <Image
      class="absolute right-[7%] top-[20%] hidden min-[1700px]:block"
      src={graphic14}
      alt=""
      loading="eager"
      width={80}
      aria-hidden="true"
    />
    <Image
      class="absolute bottom-[20%] right-[22%] hidden min-[1700px]:block"
      src={graphic11}
      alt=""
      loading="eager"
      width={100}
      aria-hidden="true"
    />
    <Image
      class="absolute bottom-0 right-[50%] hidden min-[1700px]:block"
      src={graphic15}
      alt=""
      loading="eager"
      width={100}
      aria-hidden="true"
    />
    <div class="m-auto flex max-w-[1400px] gap-12 px-8 py-24">
      <div class="flex flex-col justify-center gap-4">
        <p class="font-lilita text-5xl uppercase text-white md:text-7xl">
          <span>Lead with fun</span><br /><span>Become a</span><br /><span
            class="text-6xl md:text-8xl">Fizz owner</span
          >
        </p>
        <a href="#contact">
          <Button
            className="mt-8 w-fit bg-[#8F44E1] p-8 font-lilita text-xl uppercase tracking-wider hover:bg-[#F6BA33]"
            >Enquire Today</Button
          >
        </a>
      </div>
      <div class="hidden w-full md:block md:w-1/2">
        <FranchisingHero img={hero} alt="two staff smiling" />
      </div>
    </div>
  </div>
  <div class="relative overflow-hidden">
    <Image
      src={purpleWave}
      alt=""
      class=""
      aria-hidden="true"
      loading="eager"
    />
    <Image
      src={blueWave}
      alt=""
      class="absolute -top-0.5"
      aria-hidden="true"
      loading="eager"
    />
  </div>
  <div class="relative">
    <div class="m-auto block max-w-[1380px] px-5">
      <Image
        class="absolute left-[7%] top-[7%] hidden min-[1700px]:block"
        src={graphic12}
        alt=""
        loading="eager"
        width={80}
        aria-hidden="true"
      />
      <Image
        class="absolute bottom-[7%] right-[7%] hidden min-[1700px]:block"
        src={graphic1}
        alt=""
        loading="eager"
        width={80}
        aria-hidden="true"
      />
      <div
        class="flex flex-col items-center gap-16 py-8 md:gap-24 md:py-16 lg:flex-row"
      >
        <div class="w-full lg:w-1/2">
          <Image
            src={franchiseChild}
            alt="Child holding bath bomb and laughing"
            class="w-full rounded-3xl object-contain"
            loading="eager"
            width={800}
          />
        </div>
        <div class="w-full lg:w-1/2">
          <p class="mb-6 font-lilita text-3xl uppercase md:text-5xl">
            Fizz Kidz <span class="text-[#F24DA2]">Franchise</span>
          </p>
          <p class="mb-4 font-lilita text-2xl text-[#8F44E1] md:text-4xl">
            A proven successful business model with a team that works with you
          </p>
          <p class="font-gotham text-lg leading-8 md:text-xl">
            A Fizz Kidz franchise is an exciting powerful opportunity to combine
            your love of working with children and your entrepreneurial spirit.
            Build a business that brings smiles, sparkles, and unforgettable
            memories to <span class="font-bold"
              >your local community, with you!</span
            >
          </p>
        </div>
      </div>
    </div>
  </div>
  <div class="relative">
    <div class="m-auto block max-w-[1380px] px-5">
      <div
        class="flex flex-col-reverse items-center gap-16 py-8 md:gap-24 md:py-16 lg:flex-row"
      >
        <div class="w-full lg:w-1/2">
          <p class="mb-6 font-lilita text-3xl uppercase md:text-5xl">
            <span class="text-[#4FE16D]">Meet Talia</span> - Founder & CEO of Fizz
            Kidz
          </p>
          <p class="font-gotham text-lg leading-8 md:text-xl">
            What began 7 years ago as one tiny small studio has grown into the
            thriving values-driven company we see today. We just get it. We get
            kids, we get our customers. We get FUN! This franchising model is
            built to empower others do to the same, and share the joy and power
            of spreading fun acorss Australia. I'm deeply involved in every part
            of the business and work closely with each franchisee to support
            their journey. As a parent and a business owner, I know how
            important flexibility, purpose and community are - and that's
            exactly what owning a Fizz Kidz studio offers. If you're ready to
            own your own thriving kids studio and build something truly powerful
            — I'd love to help you get there.
          </p>
        </div>
        <div class="w-full lg:w-1/2">
          <Image
            src={talia}
            alt="Talia - founder and CEO"
            class="max-h-[500px] w-full rounded-3xl object-cover object-top"
            loading="eager"
            width={800}
          />
        </div>
      </div>
    </div>
  </div>
  <div class="relative">
    <Image
      class="absolute left-[7%] top-[7%] hidden min-[1700px]:block"
      src={graphic10}
      alt=""
      width={80}
      aria-hidden="true"
    />
    <Image
      class="absolute right-[7%] top-0 hidden min-[1700px]:block"
      src={graphic11}
      alt=""
      width={80}
      aria-hidden="true"
    />
    <section class="bg-gradient-to-t from-[#EAD6F0] to-white py-12 md:py-24">
      <div class="m-auto block max-w-[1380px] px-5 pt-4">
        <p class="text-center font-lilita text-3xl uppercase md:text-5xl">
          What <span class="text-[#8F44E1]">we do?</span>
        </p>
        <div class="my-12 grid grid-cols-1 gap-4 md:grid-cols-3">
          {
            [
              {
                content: (
                  <Fragment>
                    Birthday
                    <>
                      <br />
                      <span class="text-[#45D3F3]">Parties</span>
                    </>
                  </Fragment>
                ),
                image: birthdayParties,
                alt: "Kids smiling around a birthday cake",
              },
              {
                content: (
                  <Fragment>
                    Play Lab
                    <>
                      <br />
                      <span class="text-[#F6BA34]">(1 - 5 yrs)</span>
                    </>
                  </Fragment>
                ),
                image: playLab,
                alt: "A toddler with messy hands lost in wonderment",
              },
              {
                content: (
                  <Fragment>
                    Holiday
                    <>
                      <br />
                      <span class="text-[#F24DA2]">Programs</span>
                    </>
                  </Fragment>
                ),
                image: holidayPrograms,
                alt: "Kids arm in arm smiling",
              },
              {
                content: (
                  <Fragment>
                    After
                    <>
                      <br />
                      <span class="text-[#4FE16D]">School</span>
                    </>
                  </Fragment>
                ),
                image: afterSchool,
                alt: "A child in school pouring into a volcano and smiling",
              },
              {
                content: (
                  <Fragment>
                    Activations &
                    <>
                      <br />
                      <span class="text-[#8F44E1]">Events</span>
                    </>
                  </Fragment>
                ),
                image: activations,
                alt: "Kids in a shopping mall smiling with a Fizz Kidz staff member",
              },
              {
                content: (
                  <Fragment>
                    School &
                    <>
                      <br />
                      <span class="text-[#45D3F3]">Kindergarten</span>
                    </>
                  </Fragment>
                ),
                image: schools,
                alt: "Two kids in aprons showing off their messy hands",
              },
            ].map((item) => (
              <div class="flex flex-col rounded-lg bg-white p-4 shadow-lg">
                <Image
                  src={item.image}
                  alt={item.alt}
                  class="max-h-[350px] flex-grow rounded-lg object-cover"
                  width={700}
                />
                <p class="mt-4 text-center font-lilita text-xl uppercase tracking-wide">
                  {item.content}
                </p>
              </div>
            ))
          }
        </div>
      </div>
    </section>
  </div>
  <div class="relative">
    <Image
      class="absolute left-[7%] top-[7%] hidden min-[1700px]:block"
      src={graphic11}
      alt=""
      width={80}
      aria-hidden="true"
    />
    <Image
      class="absolute right-[7%] top-0 hidden min-[1700px]:block"
      src={graphic12}
      alt=""
      width={80}
      aria-hidden="true"
    />
    <section class="m-auto my-12 block max-w-[1380px] px-5 md:my-24">
      <p
        class="m-auto mb-16 max-w-[550px] text-center font-lilita text-3xl uppercase leading-9 tracking-wide md:text-5xl md:leading-[3.5rem]"
      >
        And the <span class="text-[#45D3F3]">numbers</span> just go up and up!
      </p>
      <div class="grid grid-cols-1 justify-items-center md:grid-cols-3">
        {
          [
            {
              heading: <span class="text-[#F24DA2]">5,000+</span>,
              content: "Birthday parties",
              image: pinkCircle,
            },
            {
              heading: <span class="text-[#4EE16D]">120,000+</span>,
              content: "Children attended Fizz Kidz studio",
              image: greenCircle,
            },
            {
              heading: <span class="text-[#F6BA33]">850+</span>,
              content: "5 Star Reviews",
              image: yellowCircle,
            },
          ].map((item) => (
            <div
              class="flex min-h-80 min-w-80 flex-col items-center justify-center gap-2 bg-contain bg-no-repeat"
              style={`background-image: url('${item.image.src}')`}
            >
              <p class="text-center font-lilita text-4xl">{item.heading}</p>
              <p class="max-w-48 text-center">{item.content}</p>
            </div>
          ))
        }
      </div>
    </section>
  </div>
  <Image src={purpleCurve} alt="" />
  <div class="bg-[#8F44E1]">
    <div class="relative">
      <Image
        class="absolute left-[7%] top-[7%] hidden min-[1700px]:block"
        src={graphic12}
        alt=""
        width={80}
        aria-hidden="true"
      />
      <Image
        class="absolute right-[7%] top-0 hidden min-[1700px]:block"
        src={graphic5}
        alt=""
        width={80}
        aria-hidden="true"
      />
      <Image
        class="absolute left-0 top-[50%] hidden min-[1700px]:block"
        src={graphic3}
        alt=""
        width={60}
        aria-hidden="true"
      />
      <div class="m-auto block max-w-[1380px] px-5">
        <p
          class="text-center font-lilita text-3xl uppercase leading-9 text-white md:text-5xl md:leading-[3.5rem]"
        >
          Why <span class="text-[#F6BA34]">Fizz</span> Kidz ?<br />
          <span class="text-[#F6BA34]">Because we're fizzing fantastic</span>
        </p>

        <div class="flex gap-24 py-12 lg:py-24">
          <div class="flex w-full flex-col gap-4 lg:w-1/2">
            {
              [
                "Proven and clear track record of roaring success",
                "Low capital investment",
                "Large opportunity for further growth",
                "Support office manages all customer enquiries",
                "We are whole heartedly committed to your success",
                "No full time staff required",
                "A model that can offer flexible work-life balance",
                "Custom-built in-house technology that makes running your studio seamless",
              ].map((item) => (
                <div class="rounded-xl bg-[#B17CEA] px-12 py-4 shadow-[2px_2px_0px_0px_rgba(255,255,255,1)]">
                  <p class="font-gotham font-medium text-white">{item}</p>
                </div>
              ))
            }
          </div>
          <div class="hidden w-1/2 items-center justify-center lg:flex">
            <div class="relative h-full w-full p-6">
              <Image
                class="absolute left-0 top-0 hidden min-[1700px]:block"
                src={graphic7}
                alt=""
                width={50}
                aria-hidden="true"
              />
              <Image
                class="absolute right-[40%] top-0 hidden min-[1700px]:block"
                src={graphic8}
                alt=""
                width={80}
                aria-hidden="true"
              />
              <Image
                class="absolute right-0 top-[50%] hidden min-[1700px]:block"
                src={graphic6}
                alt=""
                width={60}
                aria-hidden="true"
              />
              <Image
                class="absolute bottom-0 right-0 hidden min-[1700px]:block"
                src={graphic4}
                alt=""
                width={80}
                aria-hidden="true"
              />
              <Image
                class="absolute bottom-0 left-0 hidden min-[1700px]:block"
                src={graphic2}
                alt=""
                width={120}
                aria-hidden="true"
              />
              <StackedBubbles
                imgTop={topBubble}
                altTop="Fizz Kidz staff pouring soap into moulds as kids excitedly watch"
                imgBottom={hero}
                altBottom="Two Fizz Kids staff smiling"
              />
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="relative">
    <Image src={purpleCurve} alt="" class="absolute rotate-180" />
    <Image
      src={team}
      alt=""
      class="z-0 max-h-[800px] w-full object-cover object-top"
    />
    <Image src={clouds} alt="" class="absolute bottom-0" />
  </div>
  <div class="relative">
    <Image
      class="absolute left-[7%] top-0 hidden min-[1700px]:block"
      src={graphic9}
      alt=""
      width={80}
      aria-hidden="true"
    />
    <Image
      class="absolute right-[7%] top-0 hidden min-[1700px]:block"
      src={graphic12}
      alt=""
      width={80}
      aria-hidden="true"
    />
    <div class="m-auto block max-w-[1380px] px-5">
      <p
        class="mt-12 text-center font-lilita text-3xl uppercase tracking-wide md:text-5xl"
      >
        Together we are <span class="text-[#45D3F3]">stronger</span>
      </p>
      <div class="my-12 flex flex-col gap-6 md:my-24 md:flex-row">
        <div
          class="flex w-full flex-col gap-2 rounded-xl bg-[#45D3F3] p-8 md:w-1/2"
        >
          {
            [
              "Trained sales & marketing team working for you",
              "We provide you with booked jobs, not just leads you have to chase",
              "We teach you everything you need to know about all our services and how to train your staff",
              "Skip the start up pain with high-ranking website over 1200 reviews across google and facebook",
            ].map((item) => (
              <div class="flex items-center gap-4">
                <Image
                  src={check}
                  alt="check"
                  class="w-10 md:w-16"
                  width={200}
                />
                <p class="font-gotham font-medium text-white">{item}</p>
              </div>
            ))
          }
        </div>
        <div
          class="flex w-full flex-col gap-2 rounded-xl bg-[#8F44E1] p-8 md:w-1/2"
        >
          {
            [
              "Recognisable brand, with a database of customers ready to market to. You are never alone.",
              "Custom built infrastructure that manages everything from bookings, attendance management, invoicing and payroll.",
              "Full time operational support dedicated to support your recruitment and training support studio leasing guidance and support.",
            ].map((item) => (
              <div class="flex items-center gap-4">
                <Image
                  src={check}
                  alt="check"
                  class="w-10 md:w-16"
                  width={200}
                />
                <p class="font-gotham font-medium text-white">{item}</p>
              </div>
            ))
          }
        </div>
      </div>
    </div>
  </div>
  <section>
    <Image src={yellowWaveTop} alt="" />
    <div class="bg-[#F6BA33]">
      <div class="m-auto block max-w-[1380px] px-5 py-24">
        <p
          class="m-auto max-w-[600px] text-center font-lilita text-3xl uppercase md:text-5xl"
        >
          Fizz Kidz studios - the heart of <span class="text-[#8F44E1]"
            >Fizz Kidz</span
          >
        </p>
        <div class="mt-12 grid grid-cols-1 gap-8 md:grid-cols-2 lg:grid-cols-4">
          {
            [
              { image: studio1, alt: "A Fizz kids studio from the street" },
              { image: studio3, alt: "The inside of a Fizz Kids studio" },
              { image: studio2, alt: "Looking into a Fizz Kidz studio" },
              { image: studio4, alt: "The wall of a Fizz Kidz studio" },
            ].map((item) => (
              <div class="h-64 w-full overflow-hidden rounded-xl">
                <Image
                  src={item.image}
                  alt={item.alt}
                  width={800}
                  class="h-full w-full object-cover"
                />
              </div>
            ))
          }
        </div>
      </div>
    </div>
    <Image src={yellowWaveBottom} alt="" />
  </section>
  <section class="bg-gradient-to-t from-[#DAF1D7] to-white">
    <div class="m-auto block max-w-[1380px] px-5 py-24">
      <p
        class="m-auto mb-12 max-w-[800px] text-center font-lilita text-3xl uppercase leading-9 md:text-5xl md:leading-[3.5rem]"
      >
        Fizz Kidz franchisee <span class="text-[#4FE16D]">onboarding</span> journey
      </p>
      <div class="grid grid-cols-1 gap-6 sm:grid-cols-2 md:grid-cols-4">
        {
          [
            {
              number: "01",
              title: <>Enquiry</>,
              content: "Fill in the form below",
              image: step1,
              alt: "Lip balms in a bowl of bath crystals",
              color: "#8F44E1",
            },
            {
              number: "02",
              title: (
                <>
                  Phone <span class="text-[#45D3F3]">interview</span>
                </>
              ),
              content: "We will contact you by phone for an initial chat",
              image: step2,
              alt: "Bath bombs being made in a bowl",
              color: "#45D3F3",
            },
            {
              number: "03",
              title: (
                <>
                  Application <span class="text-[#8F44E1]">form</span>
                </>
              ),
              content:
                "Complete our franchise application form and non-disclosure agreement.",
              image: step3,
              alt: "Two Fizz Kids staff smiling while working",
              color: "#8F44E1",
            },
            {
              number: "04",
              title: (
                <>
                  Let's get to know{" "}
                  <span class="text-[#F6BA34]">each other</span>
                </>
              ),
              content:
                "Meet either online or in a Fizz Kidz studio with our CEO and COO.",
              image: step4,
              alt: "Kids making bubbling volcanos",
              color: "#F6BA34",
            },
            {
              number: "05",
              title: (
                <>
                  Discover the
                  <span class="text-[#F24DA2]">Fizz Kidz way</span>
                </>
              ),
              content:
