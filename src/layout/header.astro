---
import { Image } from "astro:assets";
import logo from "@/assets/images/Fizz Kidz Horizontal.png";
import NavigationMenu from "@/react-components/navigation/navigation-menu";
---

<div
  class="sticky top-0 z-50 flex items-center justify-between bg-white px-8 py-2 shadow-lg"
>
  <a href="/">
    <Image
      src={logo}
      alt="Fizz Kidz Logo"
      height={300}
      width={300}
      loading="eager"
      class="w-28 min-w-24"
    />
  </a>
  <NavigationMenu client:load />
</div>
