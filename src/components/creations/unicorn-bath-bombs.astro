---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/unicorn-bath-bombs.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="Unicorn Bath Bombs (With horns!)"
  color="blue"
/>
