---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/sparkling-lip-balm.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="Sparkling Lip Balm"
  color="purple"
/>
