---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/hendrix-glitter-shine.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Hendrix Glitter Shine"]}
  color="blue"
/>
