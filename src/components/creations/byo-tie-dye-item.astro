---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/byo-tie-dye-item.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="BYO Tie Dye Item"
  color="pink"
/>
