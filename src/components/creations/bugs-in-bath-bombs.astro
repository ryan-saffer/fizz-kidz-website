---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/bugs-in-bath-bombs.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Bugs in Bath Bombs"]}
  color="green"
/>
