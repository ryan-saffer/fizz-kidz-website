---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/ts-butterfly-soap.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="Folklore Butterfly Soap"
  color="purple"
/>
