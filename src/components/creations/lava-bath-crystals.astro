---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/lava-bath-crystals.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Lava Bath Crystals"]}
  color="purple"
/>
