---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/moon-beam-bracelets.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Moon Beam Bracelets"]}
  color="pink"
/>
