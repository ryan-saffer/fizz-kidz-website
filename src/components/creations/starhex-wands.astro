---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/starhex-wands.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Starhex Wands"]}
  color="pink"
/>
