---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/golden-slime.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Golden Slime"]}
  color="yellow"
/>
