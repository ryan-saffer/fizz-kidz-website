---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/tie-dye-rainbow-bath-salts.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Rainbow Bath Salts"]}
  color="purple"
/>
