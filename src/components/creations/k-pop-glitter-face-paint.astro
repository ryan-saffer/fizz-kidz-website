---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/k-pop-glitter-face-paint.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["K-Pop Glitter Face Paint"]}
  color="blue"
/>
