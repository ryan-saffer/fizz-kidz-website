---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/glitter-face-paint.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Glitter Face Paint"]}
  color="green"
/>
