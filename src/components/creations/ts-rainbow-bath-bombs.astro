---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/ts-rainbow-bath-bombs.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["'Lover' Rainbow Bath Bombs"]}
  color="blue"
/>
