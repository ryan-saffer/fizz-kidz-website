---
import FlipCreation from "../flip-creation.astro";

import glitterSlime from "@/assets/images/creations/string-slime.png";

type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={glitterSlime}
  text={["String Slime (Coming Soon!)"]}
  color="green"
/>
