---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/squishy-pockets.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
const textLines = "Squishy Pockets\n(Slime Inside!)".split("\n");
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={textLines}
  color="purple"
/>
