---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/tie-dye-tote-bags.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Tie Dye Tote Bags"]}
  color="blue"
/>
