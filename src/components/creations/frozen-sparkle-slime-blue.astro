---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/frozen-sparkle-slime-blue.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="Frozen Sparkle Slime"
  color="blue"
/>
