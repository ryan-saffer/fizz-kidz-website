---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/sand-slime.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="Sand Slime"
  color="yellow"
/>
