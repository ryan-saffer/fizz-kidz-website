---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/glitter-hair-gel.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<!-- Exact same as glitter hair gel, just different text -->
<FlipCreation
  flippable={flippable}
  image={creation}
  text={["Fairy Hair Gel"]}
  color="blue"
/>
