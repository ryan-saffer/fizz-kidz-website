---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/ts-face-paint.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text={["'Lover' Glitter Face Paint"]}
  color="blue"
/>
