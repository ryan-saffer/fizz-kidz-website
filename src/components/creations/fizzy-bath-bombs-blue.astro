---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/fizzy-bath-bombs-blue.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="Fizzy Bath Bombs"
  color="blue"
/>
