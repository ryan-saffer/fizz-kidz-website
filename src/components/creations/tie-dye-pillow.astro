---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/tie-dye-pillow.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="Tie Dye Pillow"
  color="green"
/>
