---
import FlipCreation from "../flip-creation.astro";

import creation from "@/assets/images/creations/fairy-slime.png";
type Props = {
  flippable?: boolean;
};

const { flippable = true } = Astro.props;
---

<FlipCreation
  flippable={flippable}
  image={creation}
  text="Fairy Slime"
  color="pink"
/>
