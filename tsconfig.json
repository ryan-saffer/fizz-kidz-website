{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/react-components/*": ["./src/components/react/*"],
      "@/react-ui/*": ["./src/components/react/ui/*"],
      "@/react-lib/*": ["./src/components/react/lib/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
